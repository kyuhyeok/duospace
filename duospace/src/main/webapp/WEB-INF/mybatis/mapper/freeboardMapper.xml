<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="freeboard">
	<insert id="insertFreeboard" parameterType="com.duospace.community.freeboard.Freeboard">
		INSERT INTO freeboard(boardnum,subject,content,writer,cmoimcode)
       	 	VALUES(SEQ_FREEBOARD.NEXTVAL,#{subject},#{content},#{writer},#{cmoimcode});
	</insert>
</mapper>